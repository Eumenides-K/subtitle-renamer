QTableView {
    background: #242424;
    outline: none;
    border: 1px solid #333;
    selection-background-color: transparent;
    alternate-background-color: transparent;
    border-radius: 8px;
}

QTableView::item {
    background: transparent;
    border: 0px;
    padding-left: 8px;
    padding-right: 8px;
    height: 32px;
}

QHeaderView {
    background-color: transparent;
}

QHeaderView::section {
    background-color: transparent;
    color: #EBEBEB;
    padding-left: 4px;
    padding-right: 4px;
    border: 1px solid #333;
    margin-top: -1px;
    margin-bottom: 2px;
    font-size: 13px;
    font-weight: normal;
}

QHeaderView::section:horizontal {
    border-bottom: 1px solid #333;
    border-left: none;
    height: 32px;
}

QHeaderView::section:horizontal:last {
    border-right: none;
}

QHeaderView::section:checked {
    background-color: transparent;
}
